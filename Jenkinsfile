node {
    stage('init') {
        print "BUILD_NUMBER: ${env.BUILD_NUMBER}"
        print "BUILD_ID: ${env.BUILD_ID}"
        print "WORKSPACE: ${env.WORKSPACE}"
        print "JENKINS_URL: ${env.JENKINS_URL}"
        print "BUILD_URL: ${env.BUILD_URL}"
        print "JOB_URL: ${env.JOB_URL}"
    }
    stage('get from SCM') {
        print "clone"
    }           
}
